    Layout = "_Layout";
}

<div class="container-fluid">
    <div class="row">
        <!-- Sidebar Column -->
        <div class="col-md-1 sidebar">
            <!-- Sidebar navigation items -->
            <div class="sidebar-item">
                <a href="#idDadosdoProduto" class="sidebar-link">
                    <i class="sidebar-icon fa-solid fa-table"></i>
                    <span class="sidebar-text">Dados do Produto</span>
                </a>
            </div>
            <div class="sidebar-item">
                <a href="#idGC" class="sidebar-link">
                    <i class="sidebar-icon fa-solid fa-list"></i>
                    <span class="sidebar-text">Gerenciamento de Categoria</span>
                </a>
            </div>
            <div class="sidebar-item">
                <a href="#idDadosTributarios" class="sidebar-link">
                    <i class="sidebar-icon fa-solid fa-square-root-variable"></i>
                    <span class="sidebar-text">Dados Tributários</span>
                </a>
            </div>
            <div class="sidebar-item">
                <a href="#idDadosLogisticos" class="sidebar-link">
                    <i class="sidebar-icon fa-solid fa-truck"></i>
                    <span class="sidebar-text">Dados Logísticos</span>
                </a>
            </div>
            <div class="sidebar-item">
                <a href="#idCadastro" class="sidebar-link">
                    <i class="sidebar-icon fa-solid fa-address-card"></i>
                    <span class="sidebar-text">Cadastro</span>
                </a>
            </div>
            <div class="sidebar-item">
                <a href="#idComentarios" class="sidebar-link">
                    <i class="sidebar-icon fa-solid fa-comments"></i>
                    <span class="sidebar-text">Comentários</span>
