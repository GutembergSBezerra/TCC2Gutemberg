            <!-- Fluxo de Trabalho -->
            <div class="row text-center">
                <div class="gap-2">
                    <a class="btn btn-success" href="GestaoProdutos" role="button">Gerenciamento de Categoria</a>
                    <span><i class="fa-solid fa-right-long" style="color: #74C0FC;"></i></span>
                    <a class="btn btn-warning" href="#" role="button">Comercial</a>
                    <span><i class="fa-solid fa-right-long" style="color: #74C0FC;"></i></span>
                    <a class="btn btn-primary" href="#" role="button">Compras</a>
                    <span><i class="fa-solid fa-right-long" style="color: #74C0FC;"></i></span>
                    <a class="btn btn-primary" href="#" role="button">Contabilidade</a>
                    <span><i class="fa-solid fa-right-long" style="color: #74C0FC;"></i></span>
                    <a class="btn btn-primary" href="#" role="button">Cadastro</a>
